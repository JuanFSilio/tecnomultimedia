void setup(){
  size(400, 400 );
  background(#BABCBC);
  
}
void draw(){
  
  //Blanco
  fill(255);
  circle(200, 180, 50);
  
//COLORES PRIMARIOS
  
  //Rojo
  fill(255, 0, 0 );
  circle(100, 100, 100);
  
  //Verde
  fill(0, 255, 0 );
  circle( 300, 100, 100);
  
  //Azul
  fill(0, 0, 255);
  circle(200, 300, 100);

//COLORES SECUNDARIOS
  
  //Amarillo
  fill(255, 255, 0);
  circle(200, 100, 50);
  
  //Violeta
  fill(225, 0, 255);
  circle(140, 200, 50);
  
  //Celeste
  fill(0, 255, 255);
  circle(260, 200, 50);
  
     
}
